@{
    ViewData["Title"] = "FilterDataList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section css{
    <link href="~/assets/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" type="text/css" />
}
<div class="card">
    <div class="card-header border-0 pt-6">
        <div class="card-title">
            <div class="d-flex align-items-center position-relative my-1">
                <span class="svg-icon svg-icon-1 position-absolute ms-6">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                        <rect opacity="0.5" x="17.0365" y="15.1223" width="8.15546" height="2" rx="1" transform="rotate(45 17.0365 15.1223)" fill="currentColor" />
                        <path d="M11 19C6.55556 19 3 15.4444 3 11C3 6.55556 6.55556 3 11 3C15.4444 3 19 6.55556 19 11C19 15.4444 15.4444 19 11 19ZM11 5C7.53333 5 5 7.53333 5 11C5 14.4667 7.53333 17 11 17C14.4667 17 17 14.4667 17 11C17 7.53333 14.4667 5 11 5Z" fill="currentColor" />
                    </svg>
                </span>
                <input type="text" data-kt-user-table-filter="search" class="form-control form-control-solid w-250px ps-14" placeholder="Filtre Ara..." />
            </div>
        </div>
        <div class="card-toolbar">
            <button type="button" class="btn btn-light-info me-3" data-kt-menu-trigger="click" data-kt-menu-placement="bottom-end">
                <span class="svg-icon svg-icon-2">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                        <path d="M19.0759 3H4.72777C3.95892 3 3.47768 3.83148 3.86067 4.49814L8.56967 12.6949C9.17923 13.7559 9.5 14.9582 9.5 16.1819V19.5072C9.5 20.2189 10.2223 20.7028 10.8805 20.432L13.8805 19.1977C14.2553 19.0435 14.5 18.6783 14.5 18.273V13.8372C14.5 12.8089 14.8171 11.8056 15.408 10.964L19.8943 4.57465C20.3596 3.912 19.8856 3 19.0759 3Z" fill="currentColor" />
                    </svg>
                </span>
                <!--end::Svg Icon-->Filtre
            </button>
            <div class="menu menu-sub menu-sub-dropdown w-300px w-md-325px" data-kt-menu="true">
                <div class="px-7 py-5">
                    <div class="fs-5 text-dark fw-bolder">Filtreler</div>
                </div>
                <div class="separator border-gray-200"></div>
                <div class="px-7 py-5" data-kt-subscription-table-filter="form">
                    <div class="mb-10">
                        <label class="form-label fs-6 fw-bold">İl:</label>
                        <select class="form-select form-select-solid fw-bolder" data-kt-select2="true" data-placeholder="Il Seçiniz" data-allow-clear="true" data-kt-subscription-table-filter="il">
                            <option></option>
                            @for (int i = 1; i < 82; i++)
                            {
                                <option value="@i">@i</option>
                            }

                        </select>
                        <hr />
                        <label class="form-label fs-6 fw-bold">İlçe:</label>
                        <select class="form-select form-select-solid fw-bolder" data-kt-select2="true" data-placeholder="Ilçe Seçiniz" data-allow-clear="true" data-kt-subscription-table-filter="ilce">
                            <option></option>
                            @for (int i = 1; i < 972; i++)
                            {
                                <option value="@i">@i</option>
                            }

                        </select>
                        <hr />
                        <label class="form-label fs-6 fw-bold">Bölge:</label>
                        <select class="form-select form-select-solid fw-bolder" data-kt-select2="true" data-placeholder="Bolge Seçiniz" data-allow-clear="true" data-kt-subscription-table-filter="bolge">
                            <option></option>
                            @for (int i = 1; i < 13; i++)
                            {
                                <option value="@i">@i</option>
                            }

                        </select>

                    </div>
                    <div class="d-flex justify-content-end">
                        <button type="reset" class="btn btn-light btn-active-light-primary fw-bold me-2 px-6" data-kt-menu-dismiss="true" data-kt-subscription-table-filter="reset">Reset</button>
                        <button type="submit" class="btn btn-primary fw-bold px-6" data-kt-menu-dismiss="true" data-kt-subscription-table-filter="filter">Apply</button>
                    </div>
                </div>
            </div>
            <div class="d-flex justify-content-end" data-kt-user-table-toolbar="base">
            </div>

            <div class="modal fade" id="kt_modal_export_users" tabindex="-1" aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered mw-650px">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h2 class="fw-bolder">Anket Genel Listesi</h2>
                            <div class="btn btn-icon btn-sm btn-active-icon-primary" data-kt-users-modal-action="close">
                                <span class="svg-icon svg-icon-1">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                        <rect opacity="0.5" x="6" y="17.3137" width="16" height="2" rx="1" transform="rotate(-45 6 17.3137)" fill="currentColor" />
                                        <rect x="7.41422" y="6" width="16" height="2" rx="1" transform="rotate(45 7.41422 6)" fill="currentColor" />
                                    </svg>
                                </span>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="card-body py-4">
        <table class="table align-middle table-row-dashed fs-6 gy-5" id="kt_table_users">
            <thead>
                <tr class="text-start text-muted fw-bolder fs-7 text-uppercase gs-0">
                    <th class="min-w-350px">SurveyUserGUID</th>
                    <th class="min-w-125px">Açık Yaş</th>
                    <th class="min-w-125px">Grup Yaş</th>
                    <th class="min-w-125px">Cinsiyet</th>
                    <th class="min-w-125px">Il</th>
                    <th class="min-w-125px">Ilçe</th>
                    <th class="min-w-125px">Bölge</th>
                    <th class="min-w-125px">HHR</th>
                    <th class="min-w-125px">Egitim GK</th>
                    <th class="min-w-125px">Egitim HR</th>
                    <th class="min-w-125px">Emekli</th>
                    <th class="min-w-125px">AMeslek GK</th>
                    <th class="min-w-125px">AMeslek HR</th>
                    <th class="min-w-125px">YMeslek GK</th>
                    <th class="min-w-125px">YMeslek HR</th>
                    <th class="min-w-125px">YMeslek Detay GK</th>
                    <th class="min-w-125px">YMeslek Detay HR</th>
                    <th class="min-w-125px">YSES</th>
                    <th class="min-w-125px">G02</th>
                    <th class="min-w-125px">G03</th>
                    <th class="min-w-125px">Timestamp</th>
                </tr>
            </thead>
            <tbody class="text-gray-600 fw-bold">
            </tbody>
        </table>
    </div>
</div>


@section js{
    <script src="~/assets/plugins/custom/datatables/datatables.bundle.js"></script>
    <script src="js/project/FilterDataLoadTable.js"></script>
}